  "next-unused": {
    "alias": {},
    "include": [
      "src/components",
      "src/components-news",
      "src/components-metals",
      "src/components-cryptos",
      "src/components-markets",
      "src/utils",
      "src/lib",
      "src/types",
      "src/styles"
    ],
    "exclude": [
      "public",
      "styles",
      "node_modules",
      ".next",
      ".github",
      ".vscode",
      ".circleci",
      ".git"
    ],
    "entrypoints": [
      "src/pages"
    ]
  }
