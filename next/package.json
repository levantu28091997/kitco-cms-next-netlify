{
  "name": "next",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "start:nextcli": "next start -p 3000",
    "start:boost": "next-boost -p 3000",
